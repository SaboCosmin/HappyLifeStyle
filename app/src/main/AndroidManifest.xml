<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hls.happylifestyle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Happy Lifestyle"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.ReplaceFoodActivity"></activity>
        <activity android:name=".Activities.InspectFoodActivity" />
        <activity android:name=".Activities.AddFoodActivity" />
        <activity android:name=".Activities.RegisterActivity" />
        <activity android:name=".Activities.MainUserActivity" />
        <activity android:name=".Activities.CreateNewMenuActivity" />
        <activity android:name=".Activities.CalculatorActivity" />
        <activity android:name=".Activities.SeeMenuActivity" />
        <activity android:name=".Activities.ProfileActivity" />
        <activity android:name=".Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>